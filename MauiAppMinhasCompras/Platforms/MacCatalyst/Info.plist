<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- O Mac App Store exige que você especifique se o aplicativo usa criptografia. -->
	<!-- Consulte https://developer.apple.com/documentation/bundleresources/information_property_list/itsappusesnonexemptencryption -->
	<!-- <key>ITSAppUsesNonExemptEncryption</key> -->
	<!-- Indique <true/> ou <false/> aqui. -->

	<!-- Especifique a categoria do seu aplicativo aqui. -->
	<!-- Consulte https://developer.apple.com/documentation/bundleresources/information_property_list/lsapplicationcategorytype -->
	<!-- <key>LSApplicationCategoryType</key> -->
	<!-- <string>public.app-category.YOUR-CATEGORY-HERE</string> -->

	<!-- Define a família de dispositivos suportados. Neste caso, 2 representa dispositivos iPad. -->
	<key>UIDeviceFamily</key>
	<array>
		<integer>2</integer>
	</array>

	<!-- Define as capacidades de hardware necessárias para o aplicativo. -->
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<!-- O aplicativo requer um dispositivo com arquitetura arm64. -->
		<string>arm64</string>
	</array>

	<!-- Define as orientações de interface suportadas para iPhone. -->
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<!-- Suporta orientação retrato. -->
		<string>UIInterfaceOrientationPortrait</string>
		<!-- Suporta orientação paisagem à esquerda. -->
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<!-- Suporta orientação paisagem à direita. -->
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>

	<!-- Define as orientações de interface suportadas para iPad. -->
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<!-- Suporta orientação retrato. -->
		<string>UIInterfaceOrientationPortrait</string>
		<!-- Suporta orientação retrato invertido. -->
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<!-- Suporta orientação paisagem à esquerda. -->
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<!-- Suporta orientação paisagem à direita. -->
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>

	<!-- Define o caminho para os recursos de ícones do aplicativo. -->
	<key>XSAppIconAssets</key>
